{
    "Version": "2012-10-17",
    "Statement": [
        {
            "Effect": "Allow",
            "Action": [
                "dynamodb:GetItem",
                "iam:GetCredentialReport",
                "iam:GenerateCredentialReport",
                "iam:ListGroupsForUser",
                "logs:CreateLogGroup",
                "logs:CreateLogStream",
                "logs:PutLogEvents",
                "ses:SendRawEmail",
                "SNS:Publish"
            ],
            "Resource": "*"
        }
    ]
}